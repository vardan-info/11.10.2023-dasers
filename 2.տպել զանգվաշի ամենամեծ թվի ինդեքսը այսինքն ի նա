      //0123456
int arr[] = {1,5 ,9, 6, 7, 10};
 int max = arr[0];
        for (int i = 0; i < 6; i++) {
            if (arr[i] > max) {//0.1.2.3.4.5.6
            max++;

            }


        }
        System.out.println(max);//6
//1 5 9 6 7 10



     //0123456
        int arr[] = {1,2,3,4,5, 7,8,9};
        int max = arr[0];
        for (int i = 0; i < 8; i++) {
            if (arr[i] > max) {//0.1.2.3.4.5.6
                max++;

            }


        }
        System.out.println(max);//6

    }
