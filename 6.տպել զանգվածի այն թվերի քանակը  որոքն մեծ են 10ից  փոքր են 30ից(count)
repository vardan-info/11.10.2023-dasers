  int[] array3 = new int[4];



        array3[0] = 10;
        array3[1] = 15;
        array3[2] = 20;
        array3[3] = 30;

        int count = 0;

        for (int i = 0; i < array3.length; i++) {
            if (array3[i] <30 && array3[i] >10) {
                count++;

            }

        }


////2

        System.out.println(count);
    }
