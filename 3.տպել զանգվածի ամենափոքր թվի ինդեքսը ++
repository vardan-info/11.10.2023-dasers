
        int arr[] = {8, 5, 9, 6, 7, 10};
        int min = arr[0];
        int index =0;
        for (int i = 0; i <6; i++) {
            if (arr[i] < min) {//0.1.2.3.4.5.6
                min=arr[i];
                index=i;
            }


        }

        System.out.println("index="+index);
       System.out.println("min="+min);

    }
}




