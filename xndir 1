        int []arr = {1,2,3,4,5,6};
        int max=arr[0];
        int min=arr[0];
        for (int i = 0; i < 6 ; i++) {//0.1.2.3.4.5
            if(arr[i] > max){
                max+=min;

            }
             if(arr[i] > min){
                min+=arr[i];

            }
             System.out.println("+" +max);
             System.out.println("- "+min);

        }
        }//1 5 9.6 7 10


 }
